generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model movies {
  id        Int      @id @default(autoincrement())
  name      String
  plataform String
  gender    String
  watched   Boolean  @default(false)
  note      Decimal? @db.Decimal
  resume    String?
}
